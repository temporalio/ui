# Local Development Environment Variables
# Copy this file to .env.local and update the values for your local setup

# Temporal Server Configuration
TEMPORAL_ADDRESS=127.0.0.1:7233
UI_PORT=8080

# Authentication Configuration
AUTH_ENABLED=true
AUTH_PROVIDER_URL=http://localhost:8080/realms/temporal-ui
AUTH_CLIENT_ID=temporal-ui
AUTH_CLIENT_SECRET=your-client-secret-here
AUTH_CALLBACK_URL=http://localhost:8080/auth/sso/callback
AUTH_SCOPES=openid,profile,email
AUTH_LABEL=Sign in with Keycloak

# Keycloak Configuration (if using Docker)
KEYCLOAK_URL=http://localhost:8080
ADMIN_PASSWORD=admin123
CALLBACK_URL=http://localhost:8080/auth/sso/callback
