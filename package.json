{
  "name": "@temporalio/ui",
  "version": "2.16.11",
  "engines": {
    "pnpm": ">=8.6.0",
    "node": ">=18.15.0"
  },
  "type": "module",
  "description": "Temporal.io UI",
  "keywords": [
    "temporal",
    "ui",
    ""
  ],
  "exports": {
    "./*.svelte": {
      "types": "./dist/*.svelte.d.ts",
      "svelte": "./dist/*.svelte"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "files": [
    "dist/**/*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "license": "MIT",
  "homepage": "https://github.com/temporalio/ui/tree/main",
  "scripts": {
    "prepare": "svelte-kit sync && esno scripts/download-temporal.ts && husky install",
    "dev": "pnpm dev:ui-server -- --open",
    "dev:ui-server": ". ./.env.ui-server && vite dev --mode ui-server",
    "dev:temporal-cli": "vite dev --mode temporal-server",
    "dev:docker": ". ./.env && VITE_API=http://localhost:8080 vite dev --mode docker",
    "build:local": "vite build",
    "build:docker": "VITE_API=http://localhost:8080 vite build",
    "build:server": "VITE_API= BUILD_PATH=server/ui/assets/local vite build",
    "temporal-server": "esno scripts/start-temporal-server.ts --codecEndpoint http://127.0.0.1:8888",
    "codec-server": "esno ./scripts/start-codec-server.ts --port 8888",
    "serve:playwright:e2e": "vite dev --mode test.e2e",
    "serve:playwright:integration": "vite dev --mode test.integration --port 3333",
    "serve:cypress": ". ./.env && VITE_TEMPORAL_UI_BUILD_TARGET=local VITE_MODE=test vite dev --mode test.cypress",
    "test": "TZ=UTC vitest",
    "test:ui": "TZ=UTC vitest --ui",
    "test:coverage": "TZ=UTC vitest run --coverage",
    "test:e2e": "PW_MODE=e2e playwright test tests/e2e",
    "test:e2e:run": "pnpm test:e2e --ui",
    "test:integration:run": "PW_MODE=integration playwright test tests/integration",
    "test:integration": "PW_MODE=integration playwright test --ui tests/integration",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore . && stylelint \"src/**/*.{css,postcss,svelte}\"",
    "lint:fix": "eslint --ignore-path .gitignore --fix",
    "format": "prettier --write --plugin-search-dir=. . && stylelint \"src/**/*.{css,postcss,svelte}\" --fix",
    "check": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json --watch",
    "cypress": "concurrently \"pnpm run serve:cypress\" \"cypress open\"",
    "cypress:run": "cypress run",
    "preview:local": "VITE_TEMPORAL_UI_BUILD_TARGET=local vite preview",
    "preview:docker": "VITE_API=http://localhost:8080 VITE_TEMPORAL_UI_BUILD_TARGET=local vite preview",
    "package": "svelte-package",
    "package:patch": "pnpm version patch && svelte-package",
    "package:minor": "pnpm version minor && svelte-package",
    "package:major": "pnpm version major && svelte-package",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "generate:locales": "esno scripts/generate-locales.ts",
    "workflows": "esno scripts/workflows.ts"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.4.0",
    "@codemirror/commands": "^6.1.3",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/language": "^6.3.2",
    "@codemirror/state": "^6.2.0",
    "@codemirror/view": "^6.7.2",
    "@crownframework/svelte-error-boundary": "^1.0.3",
    "@lezer/highlight": "^1.1.3",
    "@sveltejs/package": "^2.0.2",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "esm-env": "^1.0.0",
    "i18next": "^22.4.15",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.2.0",
    "json-beautify": "^1.1.1",
    "json-bigint": "^1.0.0",
    "just-debounce": "^1.1.0",
    "sanitize-html": "^2.8.1",
    "url-pattern": "^1.0.3",
    "uuid": "^9.0.0",
    "vis-timeline": "^7.7.2",
    "websocket-as-promised": "^2.0.1"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.7.2",
    "@babel/core": "^7.17.9",
    "@babel/preset-typescript": "^7.16.7",
    "@grpc/grpc-js": "^1.3.7",
    "@histoire/controls": "^0.16.1",
    "@histoire/plugin-svelte": "^0.16.1",
    "@histoire/shared": "^0.16.1",
    "@playwright/test": "^1.34.3",
    "@sveltejs/adapter-static": "^1.0.5",
    "@sveltejs/adapter-vercel": "^1.0.5",
    "@sveltejs/kit": "1.15.4",
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@temporalio/activity": "^1.7.4",
    "@temporalio/client": "^1.7.4",
    "@temporalio/common": "^1.7.4",
    "@temporalio/proto": "^1.7.4",
    "@temporalio/testing": "^1.6.0",
    "@temporalio/worker": "^1.7.4",
    "@temporalio/workflow": "^1.7.4",
    "@types/base-64": "^1.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/json-bigint": "^1.0.1",
    "@types/mkdirp": "^1.0.2",
    "@types/node": "^16.18.11",
    "@types/rimraf": "^3.0.2",
    "@types/sanitize-html": "^2.8.0",
    "@types/tar-fs": "^2.0.1",
    "@types/uuid": "^9.0.0",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitest/ui": "^0.16.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "babel-preset-vite": "^1.0.4",
    "c8": "^7.12.0",
    "chalk": "^4.1.2",
    "concurrently": "^7.1.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.14",
    "cypress": "^9.7.0",
    "esbuild": "^0.13.15",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-playwright": "^0.12.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-vitest": "^0.0.34",
    "esno": "^0.16.3",
    "express": "^4.18.2",
    "google-protobuf": "^3.17.3",
    "histoire": "^0.16.1",
    "husky": "^8.0.3",
    "i18next-fs-backend": "^2.1.2",
    "jest-websocket-mock": "^2.4.0",
    "jsdom": "^20.0.3",
    "lint-staged": "^13.1.0",
    "mkdirp": "^2.1.3",
    "mock-socket": "^9.1.5",
    "node-fetch": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-cli": "^9.1.0",
    "postcss-html": "^1.5.0",
    "postcss-import": "^14.1.0",
    "postcss-load-config": "^3.1.4",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "rimraf": "^4.1.2",
    "stylelint": "^15.1.0",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-standard": "^30.0.1",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "svelte-highlight": "^3.4.0",
    "svelte-loader": "^3.1.4",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.23",
    "tailwindcss": "^3.2.4",
    "tar-fs": "^2.1.1",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.82.5",
    "tslib": "^2.3.1",
    "typescript": "^4.9.5",
    "vite": "^4.0.4",
    "vite-node": "^0.23.4",
    "vitest": "^0.28.5",
    "vitest-localstorage-mock": "^0.0.1",
    "wait-port": "^1.0.4",
    "webpack": "^5.73.0",
    "yargs": "^17.7.2",
    "zx": "^7.1.1"
  },
  "peerDependencies": {
    "date-fns": "2.29.x",
    "date-fns-tz": "1.3.x",
    "websocket-as-promised": "2.0.x"
  }
}
