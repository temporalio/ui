{
  "name": "temporal-ui",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "npm run dev:local -- --open",
    "dev:local": ". ./.env && VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-kit dev",
    "dev:cloud": "VITE_TEMPORAL_UI_BUILD_TARGET=cloud svelte-kit dev",
    "build:local": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-kit build",
    "build:cloud": "VITE_TEMPORAL_UI_BUILD_TARGET=cloud svelte-kit build",
    "test": "TZ=UTC jest",
    "preview:local": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-kit preview",
    "preview:cloud": "VITE_TEMPORAL_UI_BUILD_TARGET=cloud svelte-kit preview",
    "check": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json --watch",
    "check:local": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json",
    "check:local:watch": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json --watch",
    "check:cloud": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json",
    "check:cloud:watch": "VITE_TEMPORAL_UI_BUILD_TARGET=local svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
    "cypress:open": "TEMPORAL_API_HOST=http://localhost:3000 cypress open"
  },
  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "date-fns": "^2.23.0",
    "date-fns-tz": "^1.2.2",
    "json-beautify": "^1.1.1",
    "just-debounce": "^1.1.0",
    "sanitize-html": "^2.6.1",
    "svelte-fa": "^2.4.0",
    "url-pattern": "^1.0.3",
    "websocket-as-promised": "^2.0.1"
  },
  "devDependencies": {
    "@crownframework/svelte-error-boundary": "^1.0.3",
    "@grpc/grpc-js": "^1.3.7",
    "@sveltejs/adapter-static": "^1.0.0-next.28",
    "@sveltejs/kit": "^1.0.0-next.288",
    "@temporalio/proto": "^0.3.1",
    "@types/base-64": "^1.0.0",
    "@types/jest": "^27.0.1",
    "@types/sanitize-html": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "autoprefixer": "^10.4.2",
    "babel-preset-vite": "^1.0.4",
    "chalk": "^4.1.2",
    "cssnano": "^5.0.8",
    "cypress": "^8.3.1",
    "esbuild": "^0.13.15",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "google-protobuf": "^3.17.3",
    "husky": "^7.0.2",
    "jest": "^27.1.0",
    "jest-websocket-mock": "^2.2.1",
    "mock-socket": "^9.1.0",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.0",
    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.5",
    "svelte-highlight": "^3.4.0",
    "svelte-preprocess": "^4.10.4",
    "tailwindcss": "^3.0.18",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-proto": "^1.82.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  }
}
